# Track large binary assets via Git LFS
assets/models/** filter=lfs diff=lfs merge=lfs -text
assets/anims/**  filter=lfs diff=lfs merge=lfs -text
assets/**/*.png  filter=lfs diff=lfs merge=lfs -text
assets/**/*.jpg  filter=lfs diff=lfs merge=lfs -text
assets/**/*.jpeg filter=lfs diff=lfs merge=lfs -text
assets/**/*.ktx2 filter=lfs diff=lfs merge=lfs -text
assets/**/*.bin  filter=lfs diff=lfs merge=lfs -text
assets/**/*.glb  filter=lfs diff=lfs merge=lfs -text
